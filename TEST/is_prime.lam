fun main() =
(
    let 
        val is_prime = (
            lam(x:int):bool =>
            let 
                val aux_prime= fix f(y: int): bool =>
                    if y > 1 then 
                    if x%y=0 then (1=0) else f(y - 1)
                    else (0=0) 
            in
                aux_prime(x-1)
            end
        )
    in
        let val x=13 in
        let val _ = print("isPrime(") in
        let val _ = print(x) in
        let val _ = print(") = ") in
        let val _ = print(is_prime(x)) in print("\n") end   
        end
        end
        end
        end
    end
) 
